    "def gradientDescent(gradient, start=0, rate =1, iterations =500):\n",
    "    x_old = start\n",
    "    for i in range(iterations):\n",
    "        x_new = x_old -rate * gradient(x_old)\n",
    "        if(x_new ==  x_old):\n",
    "            break \n",
    "        x_old = x_new\n",
    "    return x_new"
